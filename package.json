{
        "name": "@damuel90/nestjs-multitenancy",
        "version": "3.0.3",
        "description": "Nestjs module for multitenancy support with mongoose",
        "author": "Damuel Querales <damuel90@gmail.com>",
        "repository": {
                "type": "git",
                "url": "git+https://github.com/damuel90/nestjs-tenancy.git"
        },
        "license": "MIT",
        "private": false,
        "main": "dist/index.js",
        "types": "dist/index.d.ts",
        "scripts": {
                "lint": "eslint \"lib/**/*.ts\" --fix",
                "format": "prettier \"lib/**/*.ts\" --write",
                "build": "rm -rf dist && tsc -p tsconfig.build.json",
                "precommit": "lint-staged",
                "prepublish:npm": "npm run build",
                "publish:npm": "npm publish --access public",
                "prepublish:next": "npm run build",
                "publish:next": "npm publish --access public --tag next",
                "prerelease": "npm run build",
                "release": "release-it",
                "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
                "test": "jest --config ./tests/jest-e2e.json --runInBand",
                "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch"
        },
        "dependencies": {
                "@nestjs/common": "^10.0.0",
                "@nestjs/core": "^10.0.0",
                "@nestjs/mongoose": "^10.0.0",
                "mongoose": "^6.7.0",
                "reflect-metadata": "^0.1.13",
                "rxjs": "^7.5.0"
        },
        "devDependencies": {
                "@nestjs/cli": "^9.1.5",
                "@nestjs/platform-express": "^9.1.6",
                "@nestjs/schematics": "^9.0.3",
                "@nestjs/testing": "^9.1.6",
                "@types/express": "^4.17.14",
                "@types/jest": "^29.2.0",
                "@types/node": "18.11.5",
                "@types/supertest": "^2.0.12",
                "@typescript-eslint/eslint-plugin": "^5.41.0",
                "@typescript-eslint/parser": "^5.41.0",
                "eslint": "^8.26.0",
                "eslint-config-prettier": "8.5.0",
                "eslint-plugin-import": "2.26.0",
                "eslint-plugin-prettier": "4.2.1",
                "jest": "29.2.2",
                "prettier": "2.7.1",
                "release-it": "^15.5.0",
                "supertest": "6.3.1",
                "ts-jest": "29.0.3",
                "ts-loader": "^9.4.1",
                "ts-node": "^10.9.1",
                "tsconfig-paths": "^4.1.0",
                "typescript": "^4.8.4"
        },
        "peerDependencies": {
                "@nestjs/common": "^10.0.0",
                "@nestjs/core": "^10.0.0",
                "@nestjs/mongoose": "^10.0.0",
                "mongoose": "^6.7.0",
                "reflect-metadata": "^0.1.13",
                "rxjs": "^7.5.0"
        },
        "lint-staged": {
                "**/*.{ts,json}": []
        }
}
